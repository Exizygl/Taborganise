{
  "manifest_version": 2,
  "name": "Window Profiles",
  "version": "0.1",
  "description": "Saving and restoring windows by theme",

  "permissions": [
    "tabs",
    "windows",
    "storage"
  ],

  "background": {
    "scripts": ["background.js"]
  },

"browser_action": {
  "default_title": "Window Profiles"
},

  "options_ui": {
  "page": "dashboard.html",
  "open_in_tab": true
}

}